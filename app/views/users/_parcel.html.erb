<div class="row">
  <div class="col-md-1"><%= button_to 'match' %></div>
  <div class="col-md-3"><%= parcel.description %></div>
  <div class="col-md-3"><%= parcel.destination_address.city %></div>
  <div class="col-md-3">
  <%= parcel.deliver_by %> <br>
  <%= form_for parcel, :method => :delete do  %>
    <%= submit_tag "Delete" %>
    <% end %>
  </div>
</div>