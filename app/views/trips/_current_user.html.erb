<div class="row">
  <div class="col-md-4"></div>
  <div class="col-md-2">Parcels to Deliver</div>
  <div class="col-md-3">Destination City</div>
  <div class="col-md-3">Destination Date</div>
</div>
<% if !@reviewer %>
  <% @trips = @user.trips %>
<% end %>
<% @trips.each do |trip| %>
  <% parcels = trip.parcels %>
  <%= render partial: '/trips/summary', locals: {trip: trip, parcels: parcels} %>
<% end %>