<% if parcel.trip == nil%>
  <%= button_to 'Edit', edit_parcel_path(parcel.id), method: :get %>
<% else %>
  Delivery Status: Pending <br>
  <% if parcel.trip.driver != current_user %>
    Driver: <%= parcel.trip.driver.username %>
  <% else %>
    Sender: <%= parcel.sender.username %>
  <%end %>
<% end %>