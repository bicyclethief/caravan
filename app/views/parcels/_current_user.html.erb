<div class="row">
  <div class="col-md-2">Details</div>
  <div class="col-md-4">Description</div>
  <div class="col-md-3">Destination </div>
  <div class="col-md-3">Delivery</div>
</div>
<br>
<% if !@reviewer %>
  <% @parcels = @user.parcels %>
<% end %>

<% @parcels.each do |parcel| %>
  <% trip = parcel.trip %>
  <%= render partial: '/parcels/summary', locals: {parcel: parcel, trip: trip} %>
<% end %>